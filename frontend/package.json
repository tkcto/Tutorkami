        "autoprefixer": "^10.4.7",
        "postcss": "^8.4.14",
        "svelte": "^3.44.0",
        "svelte-check": "^2.2.6",
        "svelte-preprocess": "^4.10.1",
        "tailwindcss": "^3.0.24",
        "tslib": "^2.3.1",
        "typescript": "~4.6.2"
    },
    "type": "module",
    "dependencies": {
        "app": "file:.."
    }
