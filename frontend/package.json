{
    "name": "tutorkami-client",
    "version": "1.0.0",
    "description": "Tutorkami client",
    "scripts": {
        "development": "svelte-kit dev",
        "build": "svelte-kit build",
        "package": "svelte-kit package",
        "preview": "svelte-kit preview",
